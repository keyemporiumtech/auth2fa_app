{
  "name": "mod-app",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "ionic": "npx ionic",
    "ngstart": "npm run ng serve",
    "ngtest": "npm run ng test",
    "nglint": "npm run ng lint",
    "nge2e": "npm run ng e2e",
    "ionic::build": "npm run ionic build",
    "ionic::serve": "npm run ionic serve --browser \"chrome\"",
    "prettier": "npx prettier --check src/**/*.{ts,html}",
    "fix::prettier": "npx prettier --write src/**/*.{ts,html}",
    "eslint": "npx eslint src/**/*.{ts,html}",
    "fix::eslint": "npx eslint --fix src/**/*.{ts,html}",
    "husky": "npx jest",
    "proxy-local": "npm run ionic serve -- --browser \"chrome\" -- --proxy-config src/config/proxy/local/proxy.conf.json",
    "proxy-local-ssl": "npm run ionic serve -- --browser \"chrome\" -- --proxy-config src/config/proxy/local/proxy.conf.json --ssl",
    "proxy-prod": "npm run ionic serve -- --browser \"chrome\" -- --proxy-config src/config/proxy/prod/proxy.conf.json",
    "proxy-prod-ssl": "npm run ionic serve  -- --browser \"chrome\" -- --proxy-config src/config/proxy/prod/proxy.conf.json --ssl",
    "proxy-test": "npm run ionic serve -- --browser \"chrome\" -- --proxy-config src/config/proxy/test/proxy.conf.json",
    "proxy-test-ssl": "npm run ionic serve -- --browser \"chrome\" -- --proxy-config src/config/proxy/test/proxy.conf.json --ssl",
    "build": "npm run build::lib && npm run ionic build",
    "build-local": "npm run ionic build -- --configuration=local -- --base-href ./",
    "build-prod": "npm run ionic build -- --prod -- --base-href ./",
    "build-test": "npm run ionic build -- --configuration=test -- --base-href ./",
    "lib-kit-ionic": "barrelsby --directory \"src/app/kit-ionic/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "lib-kit": "barrelsby --directory \"src/app/kit/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "lib-oauth-social": "barrelsby --directory \"src/app/oauth-social/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "lib-rest": "barrelsby --directory \"src/app/rest/\" --delete -e \".*(^.*spec.*$).*\" -n \"index.ts\"",
    "lib-kit-ionic-lnx": "barrelsby --directory src/app/kit-ionic/ --delete -e .*(^.*spec.*$).* -n index.ts -q",
    "lib-kit-lnx": "barrelsby --directory src/app/kit/ --delete -e .*(^.*spec.*$).* -n index.ts -q",
    "lib-oauth-social-lnx": "barrelsby --directory src/app/oauth-social/ --delete -e .*(^.*spec.*$).* -n index.ts -q",
    "lib-rest-lnx": "barrelsby --directory src/app/rest/ --delete -e .*(^.*spec.*$).* -n index.ts -q",
    "build::lib": "npm run lib-kit && npm run lib-oauth-social && npm run lib-rest && npm run lib-kit-ionic",
    "build::lib-lnx": "npm run lib-kit-lnx && npm run lib-oauth-social-lnx && npm run lib-rest-lnx && npm run lib-kit-ionic-lnx",
    "serve": "npm run build::lib && ionic serve -- --browser \"chrome\"",
    "serve::local": "npm run build::lib && npm run proxy-local",
    "serve::local-ssl": "npm run build::lib && npm run proxy-local-ssl",
    "serve::prod": "npm run build::lib && npm run proxy-prod",
    "serve::prod-ssl": "npm run build::lib && npm run proxy-prod-ssl",
    "serve::test": "npm run build::lib && npm run proxy-test",
    "serve::test-ssl": "npm run build::lib && npm run proxy-test-ssl",
    "build::local": "npm run build::lib && npm run build-local",
    "build::prod": "npm run build::lib && npm run build-prod",
    "build::test": "npm run build::lib && npm run build-test",
    "build::local-lnx": "npm run build::lib-lnx && npm run build-local",
    "build::prod-lnx": "npm run build::lib-lnx && npm run build-prod",
    "build::test-lnx": "npm run build::lib-lnx && npm run build-test",
    "publish": "cd _scripts/deploy && ./deploy-local.sh",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "lint-staged": {
    "src/**/*.{ts,html}": [
      "npm run fix::prettier",
      "npm run fix::eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.1",
    "@angular/common": "^15.2.1",
    "@angular/core": "^15.2.1",
    "@angular/forms": "^15.2.1",
    "@angular/platform-browser": "^15.2.1",
    "@angular/platform-browser-dynamic": "^15.2.1",
    "@angular/router": "^15.2.1",
    "@capacitor/app": "^5.0.3",
    "@capacitor/camera": "^5.0.4",
    "@capacitor/core": "^5.0.5",
    "@capacitor/filesystem": "^5.0.4",
    "@capacitor/haptics": "^5.0.4",
    "@capacitor/keyboard": "^5.0.4",
    "@capacitor/preferences": "^5.0.4",
    "@capacitor/status-bar": "^5.0.4",
    "@ionic/angular": "^7.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "barrelsby": "^1.0.2",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-nativeaudio": "^3.0.9",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "d3-array": "^3.2.3",
    "d3-brush": "^3.0.0",
    "d3-ease": "^3.0.1",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-time-format": "^4.1.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "ng2-file-upload": "^1.4.0",
    "ng2-pdf-viewer": "^6.4.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cdk": "^15.2.1",
    "@angular/cli": "^15.2.1",
    "@angular/compiler": "^15.2.1",
    "@angular/compiler-cli": "^15.2.1",
    "@angular/language-service": "^15.2.1",
    "@capacitor/cli": "^5.0.5",
    "@cypress/schematic": "^2.5.0",
    "@ionic/angular-toolkit": "^9.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "cypress": "latest",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.8.7",
    "ts-node": "~8.3.0",
    "typescript": "~4.8.2"
  },
  "description": "An Ionic project"
}
